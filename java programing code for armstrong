import java.lang.Math;

class HelloWorld {
    public static void main(String[] args) {
        int num = 153; 
        int temp = num;
        int sum = 0;
        int digit;


        while (temp > 0) {
            digit = temp % 10;
            sum += Math.pow(digit, 3);
            temp /= 10;
        }


        if (num == sum) {
            System.out.println(num + " is an Armstrong number.");
        } else {
            System.out.println(num + " is not an Armstrong number.");
}
}
}
